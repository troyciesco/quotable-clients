{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "start:json": "json-server --watch db.json --port 3001",
    "start:nuxt": "nuxt dev",
    "dev": "npm-run-all --parallel start:nuxt",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore . --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled=true ",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.7.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/testing": "^0.1.3",
    "@types/dompurify": "^3.0.2",
    "@types/jsdom": "^21.1.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-risxss": "^2.1.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^10.11.0",
    "json-server": "^0.17.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.7.0",
    "nuxt-vitest": "^0.10.2",
    "prettier": "^3.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@pinia/nuxt": "^0.4.11",
    "dompurify": "^3.0.5",
    "jsdom": "^22.1.0",
    "pinia": "^2.1.6",
    "sass": "^1.66.1",
    "string-strip-html": "^13.4.2",
    "vuetify": "^3.3.14"
  },
  "overrides": {
    "vue": "latest"
  }
}
